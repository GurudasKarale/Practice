function Games(props){
  
  return(
      <div className="games">
        <h1>{props.name}</h1>
        <p>{props.description}</p>
      </div>
  );
}

var app=(
<div>
  <Games name="CS:GO" description="Highly competitive fps video game."/>
  <Games name="League of legends" description="Award winning MOBA game."/>
</div>
);

ReactDOM.render(app,document.querySelector('#app'));
